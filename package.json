{
  "name": "shredindex.frontend",
  "version": "2.0.0",
  "description": "The front-end for www.shredindex.com",
  "engines": {
    "node": "^16.x"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "lint": "stylelint src/**/*.scss && eslint src/",
    "storybook": "storybook dev -p 9009",
    "storybook:ci": "storybook dev -p 9009 --ci --quiet",
    "test:cypress": "cypress run",
    "test:jest": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=e2e start-test storybook:ci http://localhost:9009 test:cypress",
    "reports:copy": "mkdir reports || true && cp jest-coverage/coverage-final.json reports/coverage-jest.json && cp cypress-coverage/coverage-final.json reports/coverage-cypress.json",
    "reports:combine": "npx nyc merge reports && mkdir .nyc_output || true && mv coverage.json .nyc_output/out.json",
    "reports:coverage": "npx nyc report --reporter clover --reporter text --report-dir coverage",
    "coverage": "npm run reports:copy && npm run reports:combine && npm run reports:coverage",
    "tests": "npm run test:jest && npm run test:e2e && npm run coverage",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underflip/shredindex-frontend.git"
  },
  "homepage": "https://www.shredindex.com",
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "7.23.3",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-transform-class-static-block": "7.23.3",
    "@babel/plugin-transform-regenerator": "7.23.3",
    "@babel/plugin-transform-runtime": "7.23.3",
    "@babel/preset-env": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@cfaester/enzyme-adapter-react-18": "^0.7.1",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.12.15",
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.6",
    "@storybook/builder-webpack5": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/react-webpack5": "^7.6.6",
    "@storybook/test": "^7.6.6",
    "@svgr/webpack": "^8.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-istanbul": "^6.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.1",
    "dotenv": "^8.6.0",
    "enzyme": "^3.0.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.6.15",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.6.0",
    "image-webpack-loader": "^8.1.0",
    "istanbul-lib-coverage": "^3.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "karma": "^6.4.2",
    "karma-coverage": "^2.2.1",
    "loader": "^2.1.1",
    "mini-css-extract-plugin": "^2.5.3",
    "mock-apollo-client": "^1.2.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook": "^7.6.6",
    "storybook-addon-react-router-v6": "^2.0.10",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-loader": "^9.5.1",
    "wait-for-expect": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-remove-empty-scripts": "^1.0.4"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@apollo/react-hooks": "^4.0.0",
    "@coreui/coreui": "^4.3.0",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.2.1",
    "@coreui/react": "^4.11.0",
    "@coreui/utils": "^2.0.2",
    "@fontsource/plus-jakarta-sans": "^4.5.0",
    "@swc/wasm": "^1.3.101",
    "@types/node": "^20.10.5",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.3.1",
    "css-loader": "^6.8.1",
    "flickity": "^2.3.0",
    "graphql": "^15.6.1",
    "graphql-tag": "^2.12.5",
    "history": "^5.0.1",
    "normalize-scss": "^7.0.1",
    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flickity-component": "^4.0.7",
    "react-helmet": "^6.1.0",
    "react-intl": "^6.5.5",
    "react-markdown": "^9.0.1",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.21.1",
    "recoil": "^0.7.6",
    "sass": "^1.69.5",
    "use-query-params": "^2.2.1",
    "waait": "^1.0.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@coreui|tippy.js|perfect-scrollbar)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "coverageDirectory": "jest-coverage"
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "exclude": [
      "src/stories/coreui/*.js",
      "src/stories/index.stories.js",
      "src/js/components/Resorts/Resorts.js",
      "src/index.js"
    ]
  }
}
