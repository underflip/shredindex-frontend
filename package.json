    "react-dom": "^18.2.0",
    "react-flickity-component": "^4.0.7",
    "react-helmet": "^6.1.0",
    "react-intl": "^6.5.5",
    "react-markdown": "^9.0.1",
    "react-router": "^6.21.1",
    "react-router-dom": "^6.21.1",
    "react-select": "^5.8.0",
    "react-select-country-list": "^2.2.3",
    "recoil": "^0.7.6",
    "sass": "^1.69.5",
    "serialize-query-params": "^2.0.2",
    "use-query-params": "^2.2.1",
    "waait": "^1.0.5"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@coreui|tippy.js|perfect-scrollbar|cheerio)/)"
    ],
    "moduleNameMapper": {
      "^.+\\.svg$": "jest-svg-transformer",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "react-markdown": "<rootDir>/mocks/ReactMarkdownMock.tsx",
      "^mapbox-gl/dist/mapbox-gl.css$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/cypress"
    ],
    "coverageDirectory": "jest-coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "components/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/stories/**",
      "!src/index.js"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.json"
      }
    }
  },
  "nyc": {
    "report-dir": "cypress-coverage",
    "exclude": [
      "src/stories/coreui/*.js",
      "src/stories/index.stories.js",
      "src/js/components/Resorts/Resorts.js",
      "src/index.js"
    ]
  }
}
