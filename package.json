    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.2.1",
    "@coreui/react": "^4.11.0",
    "@coreui/utils": "1.3.1",
    "@mapbox/mapbox-gl-geocoder": "^5.0.2",
    "@swc/wasm": "^1.3.101",
    "@types/node": "^20.16.1",
    "@typescript-eslint/typescript-estree": "^8.5.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.3.1",
