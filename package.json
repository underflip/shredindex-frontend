    "next-router-mock": "^0.9.13",
    "nyc": "^15.1.0",
    "react-docgen-typescript-plugin": "^1.0.5",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.14.0",
    "storybook": "^8.2.9",
    "storybook-addon-remix-react-router": "^3.0.0",
    "stream-browserify": "^3.0.0",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-loader": "^9.5.1",
