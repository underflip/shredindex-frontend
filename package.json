{
  "name": "shredindex.frontend",
  "version": "2.0.0",
  "description": "The front-end for www.shredindex.com",
  "engines": {
    "node": "^16.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "storybook:ci": "storybook dev -p 6006 --ci --quiet",
    "test:cypress": "cypress run",
    "test:jest": "jest --coverage",
    "test:e2e": "cross-env NODE_ENV=e2e && start-test storybook:ci http://localhost:6006 test:cypress",
    "reports:copy": "mkdir -p reports && cp jest-coverage/coverage-final.json reports/coverage-jest.json && cp cypress-coverage/coverage-final.json reports/coverage-cypress.json",
    "reports:combine": "npx nyc merge reports && mkdir -p .nyc_output && mv coverage.json .nyc_output/out.json",
    "reports:coverage": "npx nyc report --reporter lcov --reporter text --report-dir coverage",
    "coverage": "npm run reports:copy && npm run reports:combine && npm run reports:coverage",
    "tests": "npm run test:jest && npm run test:e2e && npm run coverage",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underflip/shredindex-frontend.git"
  },
  "homepage": "https://www.shredindex.com",
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "7.23.3",
    "@babel/eslint-parser": "7.23.3",
